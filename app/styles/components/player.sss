.player
  display: none
  flex-basis: 60%
  flex-wrap: wrap


.player.show
  display: flex


.player-container
  position: relative
  padding-bottom: 56.25%
  width: 100%
  height: 100%
  overflow: hidden


.player-container iframe
  position: absolute
  top: 0
  left: 0
  width: 100%
  height: 100%
